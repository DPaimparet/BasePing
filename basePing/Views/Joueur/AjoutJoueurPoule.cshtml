  @using (Html.BeginForm("AjoutJoueurPoule", "Joueur"))
  {

    <label>Joueur</label>
    @Html.DropDownList("joueur", Session["listJ"] as SelectList, string.Empty, new { @class = "form-control" })
    <label>Position</label>
    <input type="number" name="pos"  required="required" />

    <label>Match gagné</label>
    <input type="number" name="matchg" min="0" max="100" required="required" />
    <label>Match perdu</label>
    <input type="number" name="matchp" min="0" max="100" required="required" />

    <input type="submit" value="Ajouter" />

  }

